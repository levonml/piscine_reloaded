#include <stdlib.h>
#include <stdio.h>

int	ft_strlen(char *po)
{
  int i;

  i = 0;
  while(po[i])
    i++;
  return(i);
}
char *ft_strdup(char *src)
{
  int i;
  char *res;
  
  res = (char*)malloc((ft_strlen(src) + 1) * sizeof(char));
  if(res == NULL)
    return("-1");
  else
    {
      i = 0;
      while(src[i])
	res[i] = src[i];
      i++;
      res[i] = '\n';
      return(res);
    }
  return(res);
}
int main(void)
{
  char *s = "kalabok";
  char **res;
  
  res[0] = ft_strdup(s);
  
   printf("%s\n", res[0]);
   printf("%s\n", s); 
  return(0);
}
